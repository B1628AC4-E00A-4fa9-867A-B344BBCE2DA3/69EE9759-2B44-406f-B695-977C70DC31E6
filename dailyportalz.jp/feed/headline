      <pubDate>Sat, 15 Apr 2023 16:00:00 +0900</pubDate>
      <link>https://dailyportalz.jp/kiji/cake-box-bag</link>
      <guid>https://dailyportalz.jp/kiji/cake-box-bag</guid>
      <author>乙幡啓子</author>
      <enclosure type="image/jpeg" length="1010171" url="https://img.dailyportalz.jp/1516/8143/3010/My_project-1.jpg"/>
